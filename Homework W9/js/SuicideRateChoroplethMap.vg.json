{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Suicide Rate in Countries in 2019",
  "width": 800,
  "height": 400,
  "projection": {"type": "equalEarth"},
  "data": {
    "url": "https://raw.githubusercontent.com/Bot1234-dotcom/3179/refs/heads/main/Homework%20W9/js/WorldMapWithGraticules.topojson",
    "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
  },
  "transform": [
    {
      "lookup": "properties.NAME_CIAWF",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/Bot1234-dotcom/3179/refs/heads/main/Homework%20W9/data/Suicide_Rate.csv"
        },
        "key": "Country",
        "fields": ["Age-standardized death rate from self-harm among both sexes"]
      }
    }
  ],
  "mark": {"type": "geoshape", "stroke": "darkgray"},
  "encoding": {
    "color": {
      "field": "Age-standardized death rate from self-harm among both sexes",
      "type": "quantitative",
      "title": "Suicides Per 100,000"
    },
    "tooltip": [
      {"field": "properties.NAME", "type": "nominal", "title": "Country"},
      {"field": "Age-standardized death rate from self-harm among both sexes", "type": "quantitative", "title": "Suicides"}
    ]
  }
}